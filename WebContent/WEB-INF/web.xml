<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>.jspServletJdbcExample</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>user.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>UserHandler</servlet-name>
    <servlet-class>handler.UserHandler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserHandler</servlet-name>
    <url-pattern>/UserHandler</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserBean</servlet-name>
    <servlet-class>bean.UserBean</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserBean</servlet-name>
    <url-pattern>/UserBean</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserDao</servlet-name>
    <servlet-class>dao.UserDao</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserDao</servlet-name>
    <url-pattern>/UserDao</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ConnectionProvider</servlet-name>
    <servlet-class>dbconnection.ConnectionProvider</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ConnectionProvider</servlet-name>
    <url-pattern>/ConnectionProvider</url-pattern>
  </servlet-mapping>
</web-app>